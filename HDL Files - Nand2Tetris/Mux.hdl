/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */

// Lucas' Version

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a= sel, b= sel, out= out0);
    Nand(a= a, b= out0, out= out1);
    Nand(a= sel, b= b, out= out2);
    Nand(a= out1, b= out2, out= out);
}