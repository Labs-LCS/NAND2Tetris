/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */

// Lucas' Version

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a= sel, b= sel, out= out0);
    Nand(a= in, b= out0, out= out1);
    Nand(a= in, b= sel, out= out2);
    Nand(a= out1, b= out1, out= a);
    Nand(a= out2, b= out2, out= b);    
}